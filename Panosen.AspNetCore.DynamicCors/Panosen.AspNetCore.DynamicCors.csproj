<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>Panosen.AspNetCore.DynamicCors</PackageId>
    <Authors>HarrisZhang</Authors>
    <Version>1.0.0</Version>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RepositoryUrl>https://github.com/panosen/aspnetcore-dynamic-cors</RepositoryUrl>
    <PackageIcon>panosen.png</PackageIcon>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Panosen AspNetCore Dynamic Cors Middleware.</Description>
    <PackageTags>Cors</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <PropertyGroup>
    <DocumentationFile>bin\$(Configuration)\Panosen.AspNetCore.DynamicCors.xml</DocumentationFile>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Cors" Version="2.2.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\panosen.png" Pack="true" PackagePath="" />
  </ItemGroup>

</Project>
